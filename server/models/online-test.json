	"projectName": {
      "type": "string",
      "default": ""
    },	
	"enableScreenRecording": {
      "type": "boolean",
      "required": false,
	  "default":false
    },
	"enableWebCamRecording": {
      "type": "boolean",
      "required": false,
	  "default":false
    },
	"isWebBasedTest": {
      "type": "boolean",
      "required": false,
	  "default":false
    },
	"GitHubUrl": {
      "type": "string",
      "default": ""
    },	
	"isLibraryTest": {
      "type": "boolean",
      "required": false,
	  "default":false
    },
	"testType": {
      "type": "string",
      "default": ""
    },
	"taskId": {
      "type": "string",
      "default": ""
    },
	"containerName": {
      "type": "string",
      "default": ""
    },
	"isBasic": {
      "type": "boolean",
      "default":false
